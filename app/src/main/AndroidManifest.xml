<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.servicio.profesional">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="app.servicio.profesional.EnviarActivity"
            android:exported="false" />
        <activity
            android:name="app.servicio.profesional.VisaActivity"
            android:exported="false" />
        <activity
            android:name="app.servicio.profesional.CitaActivity"
            android:exported="false" />
        <activity android:name="app.servicio.profesional.DetailsActivity" />
        <activity android:name="app.servicio.profesional.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>